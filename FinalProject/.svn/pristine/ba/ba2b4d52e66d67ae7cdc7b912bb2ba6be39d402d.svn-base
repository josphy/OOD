            <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_7GwqoN98EeeNHqh_gF87ow" name="ClassServer" specification="_5cm18N98EeeNHqh_gF87ow">
              <ownedParameter xmi:id="_7Gwqod98EeeNHqh_gF87ow" name="port">
