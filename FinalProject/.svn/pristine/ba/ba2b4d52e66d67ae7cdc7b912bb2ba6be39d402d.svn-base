          <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_7DjloN98EeeNHqh_gF87ow" name="refreshAvailRoom" specification="_5CBLsN98EeeNHqh_gF87ow">
            <language>java</language>
            <body>try {
			viewAdpt.setAvailRoom(user.getChatRooms());
		} catch (RemoteException e) {
			System.err.println(&quot;error refreshing room list&quot;);
			e.printStackTrace();
		}
	</body>
