            </ownedComment>
            <generalization xmi:id="_613u8N98EeeNHqh_gF87ow" general="_6opyUN98EeeNHqh_gF87ow"/>
            <ownedAttribute xmi:id="_7BS70N98EeeNHqh_gF87ow" name="sender" visibility="package" type="_61waMN98EeeNHqh_gF87ow" association="_7RAy0d98EeeNHqh_gF87ow">
