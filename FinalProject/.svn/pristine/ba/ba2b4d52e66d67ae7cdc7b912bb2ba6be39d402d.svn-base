              <language>java</language>
              <body>cmd2ModelAdpt.buildScrollableComponent(new IComponentFactory() {

			@Override
			public Component makeComponent() {
				try {
					Component label = new JLabel(host.getSender().getUserStub().getName() + &quot;: &quot;);
					Component label2 = new JLabel(host.getData().getImage());
					JPanel panel = new JPanel();
					panel.add(label);
					panel.add(label2);
					panel.setBackground(Color.WHITE);
					panel.setLayout(new BoxLayout(panel, BoxLayout.Y_AXIS));
					return panel;
				} catch (RemoteException e) {
					System.err.println(&quot;error making component&quot;);
					e.printStackTrace();
				}
				return null;
			}
			
		}, &quot;txtDialog&quot;);
		
		return null;
	</body>
