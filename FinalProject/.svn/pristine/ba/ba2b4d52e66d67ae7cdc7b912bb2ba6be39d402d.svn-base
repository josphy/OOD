				SelectEvent.ROLLOVER);&#xD;
&#xD;
		insertBeforeCompass(viewControlsLayer);&#xD;
		getWWD().addSelectListener(new ViewControlsSelectListener(getWWD(),&#xD;
				viewControlsLayer));&#xD;
	</body>
        </ownedBehavior>
        <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_7Gfk4N98EeeNHqh_gF87ow" name="setupSky" specification="_5aNpQN98EeeNHqh_gF87ow">
          <language>java</language>
          <body>// Change atmosphere SkyGradientLayer for SkyColorLayer&#xD;
		LayerList layers = getWWD().getModel().getLayers();&#xD;
		for (int i = 0; i &lt; layers.size(); i++) {&#xD;
			if (layers.get(i) instanceof SkyGradientLayer)&#xD;
				layers.set(i, new SkyColorLayer());&#xD;
		}&#xD;
	</body>
        </ownedBehavior>
        <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_7Gfk4d98EeeNHqh_gF87ow" name="setupAnnotationToggling" specification="_5aOQUN98EeeNHqh_gF87ow">
          <language>java</language>
          <body>getWWD().addSelectListener(new SelectListener() {&#xD;
			public void selected(SelectEvent event) {&#xD;
				if (event.getEventAction().equals(SelectEvent.LEFT_CLICK)) {&#xD;
					if (event.hasObjects()) {&#xD;
						Object obj = event.getTopObject();&#xD;
						if (obj instanceof ToggleAnnotation) {&#xD;
							ToggleAnnotation annotation = (ToggleAnnotation) obj;&#xD;
							annotation.toggleText();&#xD;
						}&#xD;
					}&#xD;
				}&#xD;
			}&#xD;
		});&#xD;
	</body>
        </ownedBehavior>
