            <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_7Gu1cN98EeeNHqh_gF87ow" name="getBytes" specification="_5cObcN98EeeNHqh_gF87ow">
              <ownedParameter xmi:id="_7Gu1cd98EeeNHqh_gF87ow" name="path">
                <type xmi:type="uml:PrimitiveType" href="pathmap://UML_LIBRARIES/UMLPrimitiveTypes.library.uml#String"/>
