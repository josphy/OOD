          <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_7FvW8N98EeeNHqh_gF87ow" name="start" specification="_5Vb28N98EeeNHqh_gF87ow">
            <language>java</language>
            <body>loadToggleAnnotations();
		
		Set&lt;Annotation> states = annotationMap.keySet();
		for (Annotation state:states) {
			annotations.addAnnotation(state);
		}
		
		viewAdpt.showLayer(annotations);
	</body>
