          <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_7Djlod98EeeNHqh_gF87ow" name="quit" specification="_5CCZ0N98EeeNHqh_gF87ow">
            <language>java</language>
            <body>//concurrent modification
		(new Thread() {
			@Override
			public void run() {
				Iterator&lt;IMain2MiniAdapter> miniModels =  miniMVCAdpts.values().iterator();
				while (miniModels.hasNext()) {
					miniModels.next().quitRoom();
				}
			}
		}).start();
		
		if (rmiUtils != null)
			rmiUtils.stopRMI();
		System.exit(0);
	</body>
          </ownedBehavior>
          <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_7DkMsN98EeeNHqh_gF87ow" name="deleteRoom" specification="_6elQUN98EeeNHqh_gF87ow">
